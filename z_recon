#!/bin/bash

function recon {
    mkdir /mnt/disk7/rajeev/algo_output/$1;
    SECONDS=0;
    echo running $1 >> /home/rajeev/worm/output_images/log;
    /home/rajeev/worm/worm --z $1 --edgePower -1 --useAlternate true --saveSeeds true --saveDists false --maxDistance 10 --blur 7 --saveProbs false --showSeeds true --parallelWrite true --skipRecon false;
    echo took $SECONDS seconds >> /home/rajeev/worm/output_images/log;
    echo >> /home/rajeev/worm/output_images/log;
}


for i in {1..999}
do
    recon $i
done

